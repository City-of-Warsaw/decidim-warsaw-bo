<% if has_link_to_resource? %>
  <%= link_to parsed_resource_path(vote: true), class: "card__link" do %>
    <span class="show-for-mobile"><%= voting_number %>. </span>
    <%= title %>
    <span class="show-for-mobile"> (<%= model.esog_number %>)</span>
  <% end %>
<% else %>
  <%= title %>
<% end %>